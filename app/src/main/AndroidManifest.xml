<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Activity"
    android:versionCode="1"
    android:versionName="1.0"
    xmlns:tools="http://schemas.android.com/tools">



    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />


    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" ></uses-permission>
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" ></uses-permission>
    <application

        android:allowBackup="true"
        android:icon="@drawable/tubiao"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="icon">
        <activity
            android:name="com.example.Activity.IndexActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.example.Activity.MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.example.Activity.DescribleActivity"
            android:label="@string/app_name" >
            
        </activity>
        <activity
            android:name="com.example.Activity.ArticleActivity"
            android:label="@string/app_name" >
            
        </activity>
        <activity
            android:name="com.example.Activity.CameraActivity"
            android:label="@string/app_name" >
            
        </activity>
        <activity
            android:name="com.example.Activity.WorksActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.example.Activity.ShowWorkActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.example.Activity.ShowWorksActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.example.Activity.ImageManipulationsActivity"
            android:label="@string/app_name" >

        </activity>
    </application>
    <supports-screens android:resizeable="true"
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:anyDensity="true"/>

</manifest>
